class Solution {
    public boolean isPerfectSquare(int num) {
        int st = 1;
        int end = num;
        while(st <= end){
            int mid = st+(end-st)/2;
            long value = (long)mid*mid;
            if(value == num){
                return true;
            }
            else if(value > num){
                end = mid-1;
            }
            else{
                st = mid+1;
            }
        }
        return false;
    }
}
