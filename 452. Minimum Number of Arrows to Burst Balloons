class Solution {
    public int findMinArrowShots(int[][] points) {
        Arrays.sort(points,(a,b)->{
             
           return Integer.compare(a[1],b[1]);
        });

        int c = 1;
        long end = points[0][1];
        for(int[] a : points){
            long e1 = a[0];
            long e2 = a[1];
            // System.out.println(e1);
            // System.out.println(e2);
            if(e1 > end){
                c++;
                end = e2;
            }
        }
        return c;
    }
}
