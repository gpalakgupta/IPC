class Solution {
    public int firstMissingPositive(int[] nums) {
        int n = nums.length;
        HashSet<Integer> st = new HashSet<>();
        for(int el : nums){
            st.add(el);
        } 
        int i = 1;
        while(i <= n && st.contains(i)){
            i++;
        }
        return i;
    }
}
