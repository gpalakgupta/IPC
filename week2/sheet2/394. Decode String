class Solution {
    public String decodeString(String s) {
        Stack<Integer> dg = new Stack<>();
        Stack<String> st = new Stack<>();
        int n = s.length();
        String ans = "";
        int num = 0;
        for(int i = 0; i<n; i++){
            char ch = s.charAt(i);
            if(Character.isDigit(ch)){
                num = num*10 + (ch-'0');
            }
            else if(ch == '['){
                dg.push(num);
                num = 0;
                st.push(ans);
                ans = "";
            }
            else if(ch == ']'){
                int el = dg.size() == 0 ? 1 : dg.pop();
                String str = ans.repeat(el);
                ans = st.pop();
                ans += str;
                // st.push(ans);
            }
            else{
                ans += ch;
            }
        }
        return ans;
    }
}
